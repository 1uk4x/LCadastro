<Window x:Class="UI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LCadastros - Login" Height="164" Width="265" ResizeMode="NoResize" Topmost="True" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow">
    <Grid Height="128" Width="246">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="246*" />
            <ColumnDefinition Width="12*" />
        </Grid.ColumnDefinitions>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="25,52,0,0" Name="textBox1" VerticalAlignment="Top" Width="192" Text="Senha" GotFocus="GotFocus" LostFocus="LostFocus" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="25,23,0,0" Name="comboBox1" VerticalAlignment="Top" Width="192" IsEditable="True" Text="Funcionario" LostFocus="LostFocus1">
            <ComboBoxItem IsEnabled="False" DataContext="{Binding}" IsSelected="True" />
        </ComboBox>
        <Button Content="Entrar" Height="23" HorizontalAlignment="Left" Margin="142,81,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
    </Grid>
</Window>
